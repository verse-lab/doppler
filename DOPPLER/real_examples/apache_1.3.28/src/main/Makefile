CFLAGS=$(OPTIM) $(CFLAGS1) $(EXTRA_CFLAGS)
LIBS=$(EXTRA_LIBS) $(LIBS1)
INCLUDES=$(INCLUDES1) $(INCLUDES0) $(EXTRA_INCLUDES)
LDFLAGS=$(LDFLAGS1) $(EXTRA_LDFLAGS)

LIB=  libmain.ll
HEADERS= test_char.h uri_delims.h 

OBJS= alloc.ll buff.ll \
      http_config.ll http_core.ll http_log.ll \
      http_main.ll http_protocol.ll http_request.ll http_vhost.ll \
      util.ll util_date.ll util_script.ll util_uri.ll util_md5.ll \
      rfc1413.ll

%.ll: %.c
	$(CC) -S -emit-llvm $(INCLUDES) $(CFLAGS) $< -o $@

all: $(HEADERS) $(LIB)

$(LIB): $(OBJS)
	llvm-link $(OBJS) --only-needed -o $@ -S

clean:
	rm -f *.ll $(LIB) uri_delims.h gen_uri_delims test_char.h gen_test_char

distclean: clean
	-rm -f Makefile

uri_delims.h: gen_uri_delims
	./gen_uri_delims >uri_delims.h

gen_uri_delims: gen_uri_delims.ll
	$(CC) $(CFLAGS) $(LDFLAGS) -o gen_uri_delims gen_uri_delims.ll $(LIBS)

test_char.h: gen_test_char
	./gen_test_char >test_char.h

gen_test_char: gen_test_char.ll
	$(CC) $(CFLAGS) $(LDFLAGS) -o gen_test_char gen_test_char.ll $(LIBS)

# We really don't expect end users to use this rule.  It works only with
# gcc, and rebuilds Makefile.tmpl.  You have to re-run Configure after
# using it.
depend:
	cp Makefile.tmpl Makefile.tmpl.bak \
	    && sed -ne '1,/^# DO NOT REMOVE/p' Makefile.tmpl > Makefile.new \
	    && gcc -MM $(INCLUDES) $(CFLAGS) *.c >> Makefile.new \
	    && sed -e '1,$$s: $(INCDIR)/: $$(INCDIR)/:g' \
	           -e '1,$$s: $(OSDIR)/: $$(OSDIR)/:g' Makefile.new \
		> Makefile.tmpl \
	    && rm Makefile.new

