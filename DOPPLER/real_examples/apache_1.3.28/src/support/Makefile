SRCDIR=..
EXTRA_CFLAGS=
EXTRA_LDFLAGS=
EXTRA_LIBS=
EXTRA_INCLUDES=
EXTRA_DEPS=
CC=clang-13
OSDIR=$(SRCDIR)/os/unix
INCDIR=$(SRCDIR)/include
INCLUDES0=-I$(OSDIR) -I$(INCDIR)
SHELL=/bin/sh
OS=Linux
CPP=clang-13 -E
TARGET=httpd
OPTIM=
CFLAGS1= -DLINUX=22 -DUSE_HSREGEX -DNO_DL_NEEDED
INCLUDES1=
LIBS_SHLIB=
LDFLAGS1=
MFLAGS_STATIC=--no-print-directory
REGLIB=regex/libregex.a
EXPATLIB=
RANLIB=ranlib
LIBS1=  -lm -lcrypt
