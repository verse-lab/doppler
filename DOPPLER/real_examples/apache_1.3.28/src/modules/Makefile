default: all

all clean distclean depend :: 
	@for i in $(MODULES) ""; do \
	  if [ "x$$i" != "x" ]; then \
	    echo "===> $(SDP)modules/$$i"; \
		case "x$(OS)" in \
		  xOS390 | xTPF) (cd $$i && $(MAKE) SDP='$(SDP)' OPTIM='$(OPTIM)' $@) || exit 1;; \
		              *) (cd $$i && $(MAKE) $(MFLAGS_STATIC) SDP='$(SDP)' CC='$(CC)' AUX_CFLAGS='$(CFLAGS)' RANLIB='$(RANLIB)' OPTIM='$(OPTIM)' $@) || exit 1;; \
		esac; \
		echo "<=== $(SDP)modules/$$i"; \
	  fi; \
	done

